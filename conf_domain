			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	princesmkdt.co.id.
@	IN	A	192.168.100.151
www	IN	A	192.168.100.151
ftp	IN	A	192.168.100.151
mail	IN	A	192.168.100.151
